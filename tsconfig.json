{
  "compilerOptions": {
    "allowJs": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "baseUrl": "./",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [ "dom", "dom.iterable", "es2015.promise", "es5" ],
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./assets/dist/",
    "paths": {
      "app/*": ["./assets/scripts/*"]
    },
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": ["node_modues"],
  "include": ["./assets/scripts/global.d.ts"]
}
